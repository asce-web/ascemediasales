{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^2.2",
        "drupal/antibot": "^1.3",
        "drupal/backup_migrate": "^4.1",
        "drupal/captcha": "^1.0",
        "drupal/config_update": "^1.6",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.7.0",
        "drupal/ctools": "^3.4",
        "drupal/devel": "^2.1",
        "drupal/dropzonejs": "^2.1",
        "drupal/easy_breadcrumb": "^1.12",
        "drupal/embed": "^1.3",
        "drupal/entity_browser": "^2.4",
        "drupal/entity_embed": "^1.1",
        "drupal/externalauth": "^1.2",
        "drupal/features": "^3.8",
        "drupal/field_collection": "^1.0@alpha",
        "drupal/file_browser": "^1.2",
        "drupal/filebrowser": "^2.2",
        "drupal/formblock": "^1.0@beta",
        "drupal/google_analytics": "^3.0",
        "drupal/google_tag": "^1.3",
        "drupal/image_url_formatter": "^1.0@beta",
        "drupal/imce": "^1.7",
        "drupal/ldap": "^3.0@beta",
        "drupal/magnific_popup": "^1.4",
        "drupal/menu_breadcrumb": "^1.12",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/pathauto": "^1.6",
        "drupal/pdf_reader": "1.x-dev",
        "drupal/recaptcha": "^2.5",
        "drupal/redirect": "^1.5",
        "drupal/seckit": "^1.2",
        "drupal/sharethis": "^2.0@beta",
        "drupal/simplenews": "^2.0@beta",
        "drupal/tb_megamenu": "1.x-dev",
        "drupal/token": "^1.6",
        "drupal/webform": "^5.9",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.7.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
